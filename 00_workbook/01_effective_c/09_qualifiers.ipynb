    "#include <stdio.h>\n",
    "#include <stdlib.h>\n",
    "\n",
    "int main() {\n",
    "    const int i = 5;\n",
    "    volatile int *l = malloc(sizeof(int));\n",
    "    *l = 20;\n",
    "    int x = *l;\n",
    "    *l = x;\n",
    "\n",
    "    printf(\"%d, %d\", i, *l);\n",
    "\n",
    "    return 0;\n",
    "}"
